{
  "extends": "@modern-js/tsconfig/base",
  "compilerOptions": {
    "declaration": false,
    "jsx": "preserve",
    "baseUrl": "./",
    "paths": {
     	"@/*": ["./src/*"],
			"@utils/*": ["./src/utils/*"],
			"@components/*": ["./src/components/*"],
			"@services/*": ["./src/services/*"],
			"@styles/*": ["./src/styles/*"],
			"@store/*": ["./src/store/*"]
    }
  },
  "include": ["src", "shared", "config", "modern.config.ts"]
}
